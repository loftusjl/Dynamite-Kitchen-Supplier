<div id='home'>
  <div class='jumbo'>
    <h2 class='white-text center header'>Name Here</h2>
  </div>
  <div class='container'>
    <div class='login z-depth-3'>
      <div class='row padIt'>
        <div class='col-8 offset-2'>
          <form class='mb-4 login-form' action='/api/users' method='POST' id='signup' name='signup'>
            <div class="input-field">
              <input id="username" type="text" class="validate login-bg" name='username'/>
              <label for="username">User Name</label>
            </div>
            <div class="row">
              <div class="input-field">
                <input id="password" type="password" class="validate login-bg" name='password'/>
                <label for="password">Password</label>
              </div>
            </div>
            <button id='submit' type='submit' class='btn btn-primary right login-submit' value='Sign Up'>Sign Up</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
<script src='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/js/materialize.min.js'></script>
<script type='text/javascript' src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js'></script>
<script src='/js/index.js'></script>