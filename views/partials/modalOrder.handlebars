<div class='container'>
    <table class='highlight'>
        <thead id='order-list' class='list-group'>
            <tr>
                <th>Unit of Issue</th>
                <th>Quantity</th>
                <th> Delete </th>
                <th> Add </th>
            </tr>
        </thead>
        <tbody data-id='{{this.id}}' class='list-group-item'>
            {{#each order}}
            <tr>
                <td>{{this.prodID}}</td>
                <td>{{this.olUnitofIssue}}</td>
                <td>{{this.olQuantity}}</td>
                <td>{{this.rlTitle}}</td>
                <td>{{> delete}}</td>
                <td>{{> addOne}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>