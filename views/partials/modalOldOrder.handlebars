<div class='container'>
    <table class='highlight'>
        <thead id='order-list' class='list-group'>
            <tr>
                <th>Order #</th>
                <th>Submitted</th>
                <th>Employee</th>
            </tr>
        </thead>
        <tbody data-id='{{this.id}}' class='list-group-item'>
            {{#each order}}
            <tr>
                <td>{{this.prodID}}</td>
                <td>{{this.usSupervisorID}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>